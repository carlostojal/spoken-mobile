{
  "expo": {
    "name": "SpokenNetwork",
    "slug": "spokennetwork-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.spokennetwork"
    },
    "extra": {
      "GRAPHQL_URL": "http://192.168.1.66:4000",
      "WEBSOCKET_URL": "ws://192.168.1.66:4000",
      "EXPRESS_URL": "http://192.168.1.66:8080",
      "MEDIA_ENDPOINT": "media",
      "UPLOAD_ENDPOINT": "upload",
      "POSTS_PER_PAGE": 10
    }
  }
}
